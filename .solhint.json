{
    "extends": "solhint:recommended",
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "error",
      "max-states-count": ["error", 18],
      "avoid-low-level-calls": "error",
      "reentrancy": "error",
      "not-rely-on-time": "error"
    }
  }